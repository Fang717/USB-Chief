TARGETNAME=usbchief
C_DEFINES=$(C_DEFINES) -DDBG=1
MINIMUM_NT_TARGET_VERSION=_NT_TARGET_VERSION_WIN7
KMDF_VERSION_MAJOR=1
TARGETTYPE=DRIVER
INF_NAME=usbchief
NTTARGETFILE0=$(OBJ_PATH)\$(O)\$(INF_NAME).inf
PASS0_BINPLACE=$(NTTARGETFILE0)
TARGETLIBS=$(DDK_LIB_PATH)\usbd.lib

MSC_WARNING_LEVEL=/WX /W4

MSC_WARNING_LEVEL=$(MSC_WARNING_LEVEL) /wd4201 /wd4214 /wd4127


SOURCES = usbchief.c
RUN_WPP= $(SOURCES) -km

PASS2_BINPLACE=-:DEST usbchief usbchief.sys -:DEST usbchief usbchief.inf
TARGET_DESTINATION=wdf